            <Font size="30.0" />
         </font>
      </Label>
      <TableView fx:id="score_table" layoutX="7.0" layoutY="100.0" prefHeight="200.0" prefWidth="715.0">
        <columns>
          <TableColumn fx:id="player_data" prefWidth="75.0" text="Player" />
          <TableColumn fx:id="advancement_track_data" prefWidth="154.0" text="Advancement Track" />
            <TableColumn fx:id="buildings_data" prefWidth="104.0" text="Buildings" />
            <TableColumn fx:id="reputation_data" prefWidth="112.0" text="Reputation" />
            <TableColumn fx:id="card_bonus_data" prefWidth="117.0" text="Card Bonusses" />
            <TableColumn fx:id="total_points_data" prefWidth="152.0" text="Total points" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
