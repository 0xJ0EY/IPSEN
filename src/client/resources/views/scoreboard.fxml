<AnchorPane fx:id="root" prefHeight="400.0" prefWidth="728.0" style="-fx-background-color: khaki;" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.source.controllers.ScoreboardController">
